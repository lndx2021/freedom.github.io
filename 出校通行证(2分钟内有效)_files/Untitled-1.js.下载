var t = new Date();
h = t.getHours();
if( h > 7 && h <9 )backcolor="maroon"; 
if( h >=9 && h<10 )backcolor="maroon"; 
if( h >=10 && h <11 )backcolor="teal"; 
if( h >=11 && h <12 )backcolor="olive"; 
if( h >=12 && h <13 )backcolor="purple"; 
if( h >=13 && h <14 )backcolor="gray"; 
if( h >=14 && h <15 )backcolor="teal";
if( h >=15 && h <16 )backcolor="olive";
if( h >=16 && h <17 )backcolor="purple";
if( h >=17 && h <18 )backcolor="gray";
if( h >=18 && h <19 )backcolor="teal";
if( h >=19 && h <20 )backcolor="olive";
if( h >=20 && h <21 )backcolor="purple";
if( h >=21 && h <22 )backcolor="gray"; 
if( (h >=22 && h<=24) || (h>=0 && h<=6) )backcolor="black";//夜晚黑
document.getElementById("imgBg").style.background = backcolor;
